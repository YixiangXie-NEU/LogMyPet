{
  "name": "log-my-pet",
  "version": "1.0.0",
  "description": "A platform for users to keep track of their pets' behaviors.",
  "main": "app.js",
  "type": "module",
  "repository": "git@github.com:YixiangXie-NEU/LogMyPet.git",
  "author": "Yixiang Xie & Ting Tang",
  "license": "MIT",
  "scripts": {
    "init:db": "mongoimport -d logMyPetDB -c pets --jsonArray --file ./data/pets.json",
    "install:all": "yarn && cd frontend && yarn",
    "build": "cd frontend && yarn clean && yarn build",
    "start": "yarn build && nodemon app.js",
    "dev:server": "nodemon app.js",
    "dev:client": "cd frontend && yarn dev"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.2.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongodb": "^4.11.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react-bootstrap": "^2.6.0",
    "react-loading-skeleton": "^3.1.0",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3",
    "style-loader": "^3.3.1"
  }
}
